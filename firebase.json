{
  "hosting": {
    "public": "build/web",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "headers": [
      {
        "source": "**/*.@(js|css)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "max-age=31536000"
          }
        ]
      },
      {
        "source": "**",
        "headers": [
          {
            "key": "X-Frame-Options",
            "value": "DENY"
          },
          {
            "key": "X-Content-Type-Options",
            "value": "nosniff"
          }
        ]
      }
    ]
  },
  "flutter": {
    "platforms": {
      "android": {
        "default": {
          "projectId": "wandrr-15f70",
          "appId": "1:778106737436:android:8b27ad2ddf5162891c20d9",
          "fileOutput": "android/app/google-services.json"
        }
      },
      "dart": {
        "lib/data/app/implementations/firebase_options.dart": {
          "projectId": "wandrr-15f70",
          "configurations": {
            "android": "1:778106737436:android:8b27ad2ddf5162891c20d9",
            "ios": "1:778106737436:ios:edc6c66fa007f9cf1c20d9",
            "web": "1:778106737436:web:89a5bd2a082afab71c20d9"
          }
        }
      }
    }
  },
  "emulators": {
    "auth": {
      "port": 9099
    },
    "firestore": {
      "port": 8080
    },
    "ui": {
      "enabled": true,
      "port": 4000
    }
  }
}